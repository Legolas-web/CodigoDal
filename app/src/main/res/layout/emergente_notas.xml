<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:background="@drawable/butoontres">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:padding="10dp">

        <TextView
            android:id="@+id/lblMisNotas1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SIN CALIFICAR TALLER 1"
            android:textSize="30dp"
            android:textAlignment="center"
            android:fontFamily="@font/joti_one">

        </TextView>
        <TextView
            android:id="@+id/lblMisNotas2"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SIN CALIFICAR TALLER 2"
            android:textSize="30dp"
            android:textAlignment="center"
            android:fontFamily="@font/joti_one">

        </TextView>
        <TextView
            android:id="@+id/lblMisNotas3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SIN CALIFICAR TALLER 3"
            android:textSize="30dp"
            android:textAlignment="center"
            android:fontFamily="@font/joti_one">

        </TextView>

        <TextView
            android:id="@+id/lblMisNotas4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="SIN CALIFICAR TALLER 4"
            android:textSize="30dp"
            android:textAlignment="center"
            android:fontFamily="@font/joti_one">

        </TextView>


    </LinearLayout>

</LinearLayout>